package com.xwork.DAO;


import java.sql.DriverManager;
import java.sql.Statement;

import com.xworkz.DTO.BarDTO;





public class BarDAOimpl implements BarDAO{

	

	@Override
	public boolean save(BarDTO bardto) {
		Class.forName("com.mysql.cj.jdbc.Driver");
		java.sql.Connection connection=DriverManager.getConnection("jdbc:mysql://Localhost:3306", "root", "root");
		System.out.println(connection);
		
		String insert="insert into Perfume.companyinfo values(10,' Chanel','perfume',280,'freshh')";
		Statement statement=connection.createStatement();
		int noofRowsAffected=statement.executeUpdate(insert);
		System.out.println(noofRowsAffected);
	}
		return false;
	

			
		}
}
